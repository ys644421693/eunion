  </build>
  <modules>
    <module>eunion-portals-web</module>
    <module>eunion-portals-controller</module>
    <module>eunion-portals-entity</module>
    <module>eunion-portals-service</module>
    <module>eunion-portals-service-impl</module>
    <module>eunion-portals-repository</module>
    <module>eunion-portals-dto</module>
    <module>eunion-portals-webp</module>
